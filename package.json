{
  "name": "pyrrha-boxplorer",
  "version": "1.3.3",
  "description": "Pandora Pyrrha Boxchain Explorer",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "BROWSER=none REACT_APP_USE_HOST=local npx react-app-rewired start",
    "start:pan": "BROWSER=none REACT_APP_USE_HOST=default npx react-app-rewired start",
    "start:stage": "BROWSER=none REACT_APP_USE_HOST=staging npx react-app-rewired start",
    "build:docker": "docker-compose build --no-cache",
    "start:docker": "docker-compose up -d --remove-orphans",
    "stop:docker": "docker-compose down"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pandoraboxchain/pyrrha-boxplorer.git"
  },
  "homepage": "https://pandora.network",
  "author": "Kostiantyn Smyrnov <kostysh@gmail.com>",
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.55",
    "history": "4.7.2",
    "prop-types": "15.6.2",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-loadable": "5.4.0",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "4.0.0",
    "redux-persist": "5.10.0",
    "redux-saga": "0.16.0",
    "reselect": "3.0.1",
    "semantic-ui-css": "2.3.3",
    "semantic-ui-react": "0.82.1"
  },
  "devDependencies": {
    "node-sass-chokidar": "1.3.3",
    "react-app-rewired": "2.0.0",
    "react-scripts": "2.0.0-next.3e165448",
    "sass-loader": "7.1.0"
  },
  "keywords": [
    "pandoraboxchain",
    "blockchain",
    "ai",
    "explorer",
    "boxplorer",
    "dataset",
    "kernel"
  ],
  "private": true,
  "license": "MIT",
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  }
}
